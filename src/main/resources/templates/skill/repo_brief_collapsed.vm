            <h4>
                <a href="/repository/${repo.full_name}"><strong style="margin-right: 7px">${repo.full_name}</strong></a>
                <a href="${repo.html_url}"><i class="fa fa-github"></i></a>
            </h4>
            ${repo.description}
            #parse("/templates/skill/repo_feedbackBar.vm")
        </div>
    </div>
</div>