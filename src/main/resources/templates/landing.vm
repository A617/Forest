    <style>
        #header {
            box-shadow: none;
            background-color: transparent;
            position: absolute;
            width: 100%;
        }
        .bc_gutters {
            padding: 6%;
        }
        #header .bc_gutters {
            padding-top: 30px;
            padding-bottom: 10px;
        }
        .bc_position {
            width: auto;
            padding: 0px;
            /* text-align: left; */
            margin: 0 auto;
            max-width: 1300px;
        }
        #header_left {
            display: table-cell;
            float: none;
            padding-right: 20px;
            vertical-align: middle;
            white-space: nowrap;
        }
        #header_right {
            display: table-cell;
            vertical-align: middle;
            width: 1000px;
            float: none;
        }
        #header_left #logo {
            padding-right: 5px;
            position: relative;
            top: 2px;
        }
