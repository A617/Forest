            font-size: 16px;
            line-height: 16px;
            font-weight: 900;
            color: white;
            text-transform: uppercase;
        }
        #header_right #main-menu-links {
            margin: 0px;
            padding: 0px;
            width: auto;
            list-style: none;
            text-align: right;
            position: relative;
        }
        #header_right #main-menu-links li {
            display: inline-block;
            zoom: 1;
            margin: 0px 5px;
            padding: 0px;
        }
        #header_right #main-menu-links li > a.active, #header_right #main-menu-links li.active-trail > a {
            border-bottom: 3px solid white;
        }
        #header_right #main-menu-links li > a:hover {
            border-bottom: 3px solid white;
        }
        #header_right #main-menu-links li > a {
            display: inline-block;
            zoom: 1;
            font-size: 16px;
            line-height: 16px;
            color: #ffffff;
            text-decoration: none;
            font-weight: 900;
            text-transform: uppercase;
            border-bottom: 3px solid transparent;
            padding-bottom: 4px;
        }
        a, a:visited {
            text-decoration: none;
            outline: none;
        }

        .hero {
            min-height: 900px;
            position: relative;
        }
        .bg {
            min-height: 900px;
            width: 100%;
            position: absolute;
            background: url("/images/bg.jpg") center center;
            background-size: cover;
            z-index: -1;
        }
        .hero-content {
            padding-top: 20%;
        }
        .hero-content p {
            /*font-family: "Courier New", Courier, monospace;*/
            color:#ffffff;
            font-size:18px;
